{
  "name": "@packages/db",
  "version": "0.0.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "analyze": "prisma generate --no-hints --schema=./prisma/schema.prisma",
    "build": "prisma generate --no-hints --schema=./prisma/schema.prisma",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:push": "prisma db push --schema=./prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "db:seed": "tsx ./seed.ts",
    "db:seed:auth": "tsx ./seed-auth.ts",
    "db:reset": "prisma migrate reset --schema=./prisma/schema.prisma",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.0",
    "@prisma/adapter-neon": "6.4.1",
    "@prisma/client": "6.4.1",
    "@t3-oss/env-nextjs": "^0.13.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "server-only": "^0.0.1",
    "undici": "^7.10.0",
    "ws": "^8.18.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@tooling/typescript-config": "workspace:*",
    "@types/node": "22.15.21",
    "@types/ws": "^8.18.1",
    "bcryptjs": "^2.4.3",
    "bufferutil": "^4.0.9",
    "prisma": "6.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  }
}
